// return the most frequently occuring number in array(mode)
// if multiple modes, return first in array
// if no modes return -1

function ArrayChall(arr) {
    // if no duplicate numbers, return -1
    let toFindDuplicates = arr => arr.filter((item, index) => arr.indexOf(item) !== index)
    let duplicates = toFindDuplicates(arr);
    console.log(duplicates);
    if (duplicates == ''){
        console.log('-1');
    } else {
        console.log(duplicates[0])
        return duplicates[0];
    };

    console.log(duplicates)

    return
    // this result doesn't check for the MODE(most duplicates), just that there are duplicates
};

ArrayChall([5,5,2,2,1])